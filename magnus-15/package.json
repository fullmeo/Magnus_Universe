{
  "name": "magnus-15-orchestrator",
  "version": "15.0.0",
  "description": "Multi-agent music composition orchestrator with Claude Sonnet 4.5 and 8th Principle convergence",
  "main": "magnus-orchestrator.js",
  "scripts": {
    "start": "node magnus-orchestrator.js",
    "dev": "DEBUG=true node magnus-orchestrator.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint magnus-orchestrator.js",
    "docker:build": "docker build -t magnus-15-orchestrator .",
    "docker:run": "docker run -p 3001:3001 --env-file .env magnus-15-orchestrator"
  },
  "keywords": [
    "music",
    "composition",
    "ai",
    "claude",
    "orchestrator",
    "multi-agent",
    "magnus"
  ],
  "author": "Serigne DIAGNE",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/magnus-15-orchestrator"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "magnus-orchestrator.js"
    ]
  }
}
